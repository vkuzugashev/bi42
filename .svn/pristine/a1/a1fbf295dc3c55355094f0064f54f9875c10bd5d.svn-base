@model Bi42.Models.Contract
@{
    ViewBag.Title = "Контракт";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Текст контракта<small>, обновление: @Model.PublishTime</small></h4>

<div class="row">
        <div class="col-md-12">
            <div class="well">
                @Model.Description
            </div>
        </div>
</div>

<div class="row">&nbsp;</div>
    <div class="row">
        <div class="col-md-12">
            @Html.ActionLink("Назад", "Details", "Order", new { orderid=Model.OrderID }, new { @class = "btn btn-default" })
        </div>
    </div>
</div>

